/**
 * 16 bit multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
    <% 16.times do |i| %>
    Mux(a=a[<%=i%>],b=b[<%=i%>],sel=sel,out=out[<%=i%>]);
	<% end %>
}
